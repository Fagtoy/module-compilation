BIN=/home/yang/sdo_analysis/bin
cd $BIN
source activate
source configure.sh
rundownloadgithub.sh
echo Starting runYANGGenereicstats-daily.sh
runYANGgenericstats-daily.sh
echo Starting runIETFstats.sh
runIETFstats.sh
echo Starting rename-file-backup.py
mkdir -p $BACKUPDIR
rename-file-backup.py --documentpath=$WEB_PRIVATE/ --backuppath=$BACKUPDIR/
echo Starting runGenerateMainPage.sh
runGenerateMainPage.sh
echo Starting runGenerateFiguresAndStats.sh
runGenerateFiguresAndStats.sh

# This simple files for other Yang Catalog to know which files have been created
cd $WEB_PRIVATE
ls *.json > json_links
